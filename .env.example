# Example environment variables for Diarrhizer
# Copy to `.env` (do NOT commit `.env`).

# Hugging Face token (required for diarization / gated models)
HF_TOKEN=hf_your_token_here

# Optional: default device ("cpu" or "cuda")
DIARRHIZER_DEVICE=cuda

# Optional: default output directory
DIARRHIZER_OUT=./out
